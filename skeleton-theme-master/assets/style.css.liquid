
/* ================ */
/* =  Global CSS  = */
/* ================ */

h1, h2, h3, h4, h5, h6 {color:{{settings.header_color}};font-family:'{{ settings.header_font | remove: 'Google+' | replace: '+', ' ' }}';}
body{background-color:{{settings.shop_bg_color}};}
body,p,strong{font-family:'{{ settings.base_font | remove: 'Google+' | replace: '+', ' ' }}';color:{{settings.body_font_color}};}
img{max-width:100%; height: auto;}
a{color:{{settings.link_color}};}
a:hover{opacity:0.7;}
/* Hiding placeholder text on focus in webkit, thanks to http://www.kolodvor.net/2012/03/23/webkit-placeholder-attribute-behavior */
input:focus::-webkit-input-placeholder, textarea:focus::-webkit-input-placeholder { color:transparent; }
/* Contain floats: nicolasgallagher.com/micro-clearfix-hack/ */ 
.clearfix:before, .clearfix:after { content: ""; display: table; }
.clearfix:after { clear: both; }
.clearfix { zoom: 1; }
.hidden { display: none; }
.left { float: left; }
.right { float: right; }
.boxify{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;} 
.table{display: table !important;}
.table-cell { display: table-cell !important;  vertical-align: middle;}
.no-border{border: none !important; }

hr.divider{width:100%;display:block;height:1px;border-top: 1px solid #e8e8e8;margin:30px 0;}

.show-mobile-only{display:none !important;}


/**** Buttons ****/

.btn{background-color:transparent;font-family: Helvetica, Arial, sans-serif; height: 40px;line-height: 40px;padding:0 15px;border:none;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;font-size:14px;font-weight:bold;display:inline-block;}
.btn:hover{opacity:0.8;}

.btn-colour{color:white !important;background-color: {{settings.shop_btn_color}};}
.btn.subtle{color:{{settings.shop_btn_color}}; border:1px solid {{settings.shop_btn_color}}; }
input.btn[type="submit"]{font-weight:bold;}


/* ================ */
/* = theme.liquid = */
/* ================ */


/**** Toolbar ****/

.toolbar{height:55px; width:100%; border-bottom:1px solid {{settings.border_color}};}
.toolbar,.toolbar a{color:{{settings.toolbar_text_color}};}
.toolbar-left{float:left;}
.toolbar-left input{margin:0; border-radius:15px; background: #fff url("{{ 'search.png' | asset_url }}") 148px 9px no-repeat; padding: 6px 25px 6px 10px; width:135px; }
.toolbar-right{float:right; padding-top:6px; font-size:13px;}
.toolbar-right a:hover{text-decoration:underline;}
.toolbar-right i,.cart-link, .customer-links{display:inline-block;}
.toolbar-right i{margin-right:3px;}
.cart-link{margin-left:20px;}

/**** Header ****/
.header{margin-bottom:0;}
.header-title{font-weight: bold; 	padding: 60px 0; margin-bottom:0; text-align:center; line-height:1; }
.header-title a,.header-title a:hover{color:#000;}
.header-logo{display:block;margin:10px auto;}


/**** Header social icons ****/
.social-icons li{display:inline-block;}
.social-icons a{background: url("{{ 'social_icons.png' | asset_url }}") top left; display:inline-block; width:32px;height:32px;}

.fb a{background-position: -32px 0;}
.yt a{background-position: -64px 0;}
.rss a{background-position: -96px 0;}
.ins a{background-position: -128px 0;}
.pin a{background-position: -160px 0;}
.vim a{background-position: -192px 0;}
.tum a{background-position: -224px 0;}
.gp a{background-position: -256px 0;}

.tw a:hover{background-position:0 32px}
.fb a:hover{background-position: -32px 32px;}
.yt a:hover{background-position: -64px 32px;}
.rss a:hover{background-position: -96px 32px;}
.ins a:hover{background-position: -128px 32px;}
.pin a:hover{background-position: -160px 32px;}
.vim a:hover{background-position: -192px 32px;}
.tum a:hover{background-position: -224px 32px;}
.gp a:hover{background-position: -256px 32px;}

/**** Nav Bar ****/

nav{font-weight:bold; border-top:1px solid {{settings.border_color}}; border-bottom: 1px solid {{settings.border_color}};}
nav a{color:{{settings.nav_link_color}};}
nav li, nav ul{margin-bottom:0;}
.dropdown{/*margin:15px 0px 0px -18px; padding:0px; */}
.dropdown ul{display:none;background-color:white;border: 1px solid {{settings.border_color}}};font-weight:normal;}
.dropdown ul li{width:250px;padding: 15px 18px 15px 18px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;margin-bottom:0px;}
.m-nav-bar{display:none;}
.mobile{display:none;}


.container {padding:0;}

#main{margin-top:0px;}
#main > #sidebar, #main > .twelve, #main > .row{margin: 60px 0;}
#main > .twelve{padding-right:20px;}




/**** Side Bar ****/

#sidebar h6 {margin-bottom:5px;line-height:1.4em;}
#sidebar .sidebar-post{margin-bottom:30px; padding-bottom:30px; border-bottom:1px solid {{settings.border_color}};}



/**** Footer  ****/

#overview .article-title{margin-bottom:10px;}
#o-inner{text-align: left; border-radius: 12px;	background-color: #f2f2f2;padding: 26px; border: 1px solid #DDDBDB; margin-bottom:15px;}
.overview-third{width:278px;display:inline-block;float:left;margin-left:26px;}
.o-first{margin-left:0px;}
#o-link{margin-bottom:0px;}
.o-field{width:100%;}
#overview input[type="text"]{width:100%;-webkit-box-sizing: border-box; border: 1px solid #DDDBDB; box-sizing: border-box;}
#subscribe{margin-bottom:0px;}
.f-copy{text-align:right;}
.footer-inner{margin-bottom:30px;}
.footer-inner,.footer-inner a, .footer-inner p{color:#959595; font-size:12px;}
.footer-inner a:hover{text-decoration:underline;}
#footer-payment-methods{text-align:center; margin-bottom:30px;}
.f-links a{display:inline-block; margin-right:15px;}

/* Custom select */

.custom-select{margin:0 0 10px 10px; background-color:white; color:green; border:1px solid #ccc;padding:5px 5px 5px 10px;border-radius:5px;color:#777;}
.custom-select:after{font-family: FontAwesome; content: "\F078";padding-right:5px;}
.custom-selectHover{border: 1px solid #aaa; color: #444;}
.custom-selectFocus{border: 1px solid #aaa; color: #444;-webkit-box-shadow: 0 0 3px rgba(0,0,0,.2);box-shadow: 0 0 3px rgba(0,0,0,.2);}



/* ================ */
/* = index.liquid = */
/* ================ */




/* ================== */
/* = product.liquid = */
/* ================== */

/*** Product images ***/

#product-photos{margin-bottom:30px;}
#product-photo-container{max-width:100%;max-height:500px;}
#product-photo-container img{max-width:100%; max-height:500px;display:block;margin:0 auto;}
#product-photo-thumbs li{width:31.33%; margin:0 3% 3% 0; }
#product-photo-thumbs img{}
.product-last-thumb{margin-right:0 !important;}
.mobile-title{display:none; text-align:center; }

/*** Product details ***/

#product-details{text-align:justify; margin: 0 0 60px 0;}
#product-description .title{margin-bottom:30px;}
#product-variants{ border-top: 1px solid #e8e8e8;border-bottom: 1px solid #e8e8e8; padding-top:30px; margin-bottom:30px;}

.selector-wrapper{margin-bottom:30px;}
.selector-wrapper label{font-weight:bold;}
.selector-wrapper label, .selector-wrapper select,.selector-wrapper span{margin:0;}
#product-price{margin-bottom:30px;}
.product-price{font-size:36px;font-weight:bold; vertical-align:text-top; }
.product-compare-price{font-size:18px;font-weight:normal;padding-left:3px;color:grey; vertical-align:bottom;}




/* ==================== */
/* = collection.liquid = */
/* ==================== */

.product-tags{text-align:right;}
ul.grid li { float: left; list-style-type: none; }
.prod-image-wrap, .coll-image-wrap{padding:10px;height:300px;}
.prod-image-wrap a, .coll-image-wrap a{width:100%;}
.prod-image-wrap a img, .coll-image-wrap a img{max-height:100%;max-width:100%;margin:0 auto;display:block;}
.row-one{margin-top:0px;}
.prod-grid-item,.coll-grid-item{margin:25px 25px 25px 25px;}
.first-prod{margin-left:0px;margin-right:25px;}
.last-prod{margin-left:25px;margin-right:0px;}

.prod-caption,.coll-caption{text-align:center;}
.prod-title,.coll-title{font-weight:bold;}

div.tag-block{display:block;clear:both;width:100%;}
div.tag-right{display:block;float:right;}
.product-tags{margin:0 10px 5px 10px;display:inline-block;}
.product-tags .customSelect{text-align:left;}
#collection-tags{display:inline-block;}


.pagination{text-align:center;}

/* =============== */
/* = cart.liquid = */
/* =============== */

#cart-main .ca-prod-image{width:90px;height:auto;}
#cartform > table{width:100%;}
#cartform tr{text-align:left;border-top: 1px solid #e6e4e5;}
#cartform td, #cartform th{padding:20px 20px 20px 20px;vertical-align:middle;}
#cartform a.product-image{width:25%;float:left}
#cartform img{max-width:100%;float:right;}
#cartform a.product-info{width:70%;margin-left:5%;float:left}

	.overview-third{width:214px;}
	.f-copy{clear:both}
	.mobile .customSelect{margin:30px 0; width:100% !important;}.

	.prod-image-wrap{width:236px;height:236px;}
	.mobile .customSelect{margin:30px 0; width:100% !important;}
}


/* Mobile Portrait */

@media only screen and (max-width: 479px)  {
	.prod-image-wrap{width:300px;height:300px;}

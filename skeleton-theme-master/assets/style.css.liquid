.blog-article{margin-bottom:30px; border-bottom: 1px solid #e8e8e8;} /* No padding-bottom because rte content has padding-bottom of 30px */
.blog-article-title{font-size:25px !important;font-weight:bold; margin-bottom: 5px;}
.blog-article-date{display:inline-block; color: #666;font-weight: normal;}
.blog-article p{font-size:13px;line-height:1.52em;color:black;}
.blog-article-date,.blog-article-meta-comments{font-size:12px;}
.blog-article-metadata{margin-bottom:30px;}
.content{padding-top:20px;}
.sidebar-tag{border-bottom:1px solid #e8e8e8; margin-bottom: 15px; padding-bottom: 15px;}


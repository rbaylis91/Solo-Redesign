.blog-article-title{font-size:18px;padding-top:20px;font-weight:bold;margin-bottom:0px;line-height:24px;}
.blog-article-date{color: #666;font-size: 13px;font-weight: normal;padding: 0px 0 5px;}
.blog-article p{font-size:13px;line-height:1.52em;color:black;}



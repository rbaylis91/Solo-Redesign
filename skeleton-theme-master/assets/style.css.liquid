
/* ================ */
/* =  Global CSS  = */
/* ================ */

h1, h2, h3, h4, h5, h6 {color:#2c2f2f;}
body{font-family: Helvetica, Arial, sans-serif;}
img{max-width:100%; height: auto;}
/* Hiding placeholder text on focus in webkit, thanks to http://www.kolodvor.net/2012/03/23/webkit-placeholder-attribute-behavior */
input:focus::-webkit-input-placeholder, textarea:focus::-webkit-input-placeholder { color:transparent; }
/* Contain floats: nicolasgallagher.com/micro-clearfix-hack/ */ 
.clearfix:before, .clearfix:after { content: ""; display: table; }
.clearfix:after { clear: both; }
.clearfix { zoom: 1; }
.hidden { display: none; }
.left { float: left; }
.right { float: right; }
.boxify{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;} 

hr.divider{width:100%;display:block;height:1px;border-top: 1px solid #ddd;margin:40px 0;}

/**** Buttons ****/
.btn{font-family: Helvetica, Arial, sans-serif;margin:5px 5px 5px 0;height: 40px;line-height: 40px;padding:0 15px;border:none;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;font-size:14px;font-weight:bold;display:inline-block;}
.btn:hover{opacity:0.8;}

.btn-colour{color:white !important;background-color: #abcf2c;}
.btn-colour{color:white !important;}

input.btn[type="submit"]{font-weight:normal !important;}

/* ================ */
/* = theme.liquid = */
/* ================ */


/**** Header ****/
.header{padding:60px 0; margin-bottom:0; position: relative;}
.header-title{ font-size: 3em; font-weight: bold; 	padding: 0; margin-bottom:0;}
.header-widget{position:absolute; top:0; right:0;}


/**** Header social icons ****/
.social-icons li{display:inline-block;}
.social-icons a{background: url("{{ 'social_icons.png' | asset_url }}") top left; display:inline-block; width:32px;height:32px;}

.fb a{background-position: -32px 0;}
.yt a{background-position: -64px 0;}
.rss a{background-position: -96px 0;}
.ins a{background-position: -128px 0;}
.pin a{background-position: -160px 0;}
.vim a{background-position: -192px 0;}
.tum a{background-position: -224px 0;}
.gp a{background-position: -256px 0;}

.tw a:hover{background-position:0 32px}
.fb a:hover{background-position: -32px 32px;}
.yt a:hover{background-position: -64px 32px;}
.rss a:hover{background-position: -96px 32px;}
.ins a:hover{background-position: -128px 32px;}
.pin a:hover{background-position: -160px 32px;}
.vim a:hover{background-position: -192px 32px;}
.tum a:hover{background-position: -224px 32px;}
.gp a:hover{background-position: -256px 32px;}

/**** Nav Bar ****/

nav{font-weight:bold; border-top:1px solid #ddd; border-bottom: 1px solid #ddd;}
nav li, nav ul{margin-bottom:0;}
.dropdown{/*margin:15px 0px 0px -18px; padding:0px; */}
.dropdown ul{display:none;background-color:white;border: 1px solid #EBEBEB;font-weight:normal;}
.dropdown ul li{width:250px;padding: 15px 18px 15px 18px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;margin-bottom:0px;}
.m-nav-bar{display:none;}
.mobile{display:none;}


.title{ margin:40px 0 80px 0; text-align:center;}
.container {padding:0;}

#main{margin-top:0px;}
#main > #sidebar, #main > .twelve, #main > .row{margin-top:40px;margin-bottom:40px;}
#main > .twelve{padding-right:20px;}




/**** Side Bar ****/

#sidebar h6 {margin-bottom:5px;line-height:1.4em;}
#sidebar .sidebar-post{margin-bottom:28px;}

/**** Footer  ****/

footer{margin-top:26px;}
.f-links{float:left;}
.f-l-item{margin-right:26px;}

#overview .article-title{margin-bottom:10px;}
#o-inner{text-align: left; border-radius: 12px;	background-color: #f2f2f2;padding: 26px; border: 1px solid #DDDBDB;}
.overview-third{width:278px;display:inline-block;float:left;margin-left:26px;}
.o-first{margin-left:0px;}
#o-link{margin-bottom:0px;}
.o-field{width:100%;}
#overview input[type="text"]{width:100%;-webkit-box-sizing: border-box; border: 1px solid #DDDBDB; box-sizing: border-box;}
#subscribe{margin-bottom:0px;}



/* ================ */
/* = index.liquid = */
/* ================ */




/* ================== */
/* = product.liquid = */
/* ================== */


#product-details{text-align:justify;margin-left:0px;margin-right:0px;}
#product-details h1, #product-details h2{display:block;text-align:center}

#product-description{padding:0px 20px 0px 20px}
#product-photo-thumbs li{width:27%; height:auto;padding:3%;}

.selector-wrapper{margin-bottom:10px;}
.selector-wrapper > label{margin-bottom:6px;}
#product-variants{padding-bottom: 10px;padding-top: 10px;border-top: 1px solid #ddd;border-bottom: 1px solid #ddd;}
#product-price{margin-top:20px;}
.selector-wrapper select{width:100px;}
.product-price{font-size:36px;font-weight:bold;}
.product-compare-price{font-size:18px;font-weight:normal;padding-left:3px;color:grey;}






/* ==================== */
/* = collection.liqid = */
/* ==================== */


ul.grid li { float: left; list-style-type: none; }
.prod-image-wrap{padding:10px;}
.prod-image-wrap a{width:100%;}
.prod-image-wrap a img{width:inherit;overflow:hidden;height:auto;}
.row-one{margin-top:0px;}
.prod-grid-item{margin:25px 25px 25px 25px;}
.first-prod{margin-left:0px;margin-right:25px;}
.last-prod{margin-left:25px;margin-right:0px;}

.prod-caption{text-align:center;}







/* =============== */
/* = cart.liquid = */
/* =============== */

#cart-main .ca-prod-image{width:90px;height:auto;}
#cartform > table{width:100%;}
#cartform tr:hover{opacity:1.0 !important;}
#cartform tr{text-align:left;border-top: 1px solid #e6e4e5;opacity:0.8 !important;}
#cartform td, #cartform th{padding:20px 20px 20px 20px;vertical-align:middle;}
#cartform a.product-image{width:25%;float:left}
#cartform img{max-width:100%;float:right;}
#cartform a.product-info{width:70%;margin-left:5%;float:left}
#cartform strong, #cartform span{width:100%;display:block;clear:both;}
#cartform .qty input{margin-bottom:0px;width:30px;}
#cartform .remove > .cart:before{font-family: "FontAwesome";content:"\F05E";display:inline-block;font-size:12px;color:black;margin-right:3%;opacity:0.4;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=40)";}
#cartform .remove:hover > .cart:before{opacity:1.0;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";}
#cartform .remove{width:25%;text-align:left;}


/* =============== */
/* = blog.liquid = */
/* =============== */
#blog-main{margin:0px}
.blog-article-title{font-size:40px;font-weight:bold;line-height:1.0em;}
.blog-article-date{display:inline-block;float:left;color: #666;font-weight: normal;padding: 0px 0 5px;}
.blog-article p{font-size:13px;line-height:1.52em;color:black;}
.blog-article-meta-comments a{float:left;color: #666;margin-left:10px;font-weight:normal;}
.blog-article-meta-tags{margin-bottom:20px;}
.blog-article-meta-tags a{font-style:italic;color:#666;}



/* ======= */
/* = RTE = */
/* ======= */
.rte table{width:100%;margin-bottom:20px;}
.rte td{padding:10px;border: 1px solid #ddd;}


/* =================== */
/* =  article.liquid = */
/* =================== */
#comments .comment-author{margin-bottom:0px;}
#comments .comment-date{margin-bottom:10px;font-size:12px;color: #666;}
li.comment{margin-bottom:40px;}
#comments label{font-weight:normal;}
#comments .comment-heading{margin-bottom:30px;}
#comments .text-input{width:420px;}



/* ================= */
/* = search.liquid = */
/* ================= */
.result-image{margin:0 10px  0px 0;max-width:20%;}
.result-image img{max-width:100%;max-height:300px;margin:0 auto;display:block;}

#search{margin:0px;}
#search input.search-box{height:40px;width:420px;display:inline-block;float:left;moz-border-radius: 6px; -moz-border-radius:6px;border-radius: 6px;padding-left:10px; }
#search .search-submit{float:left;margin:0px 0px 0px 10px;}

/* ===================== */
/* = Customer Accounts = */
/* ===================== */
#addresses h4{margin-top:20px}
#customer label, #account label{font-weight:normal}
#customer #forgot_password{margin-bottom:10px}
#customer input[type=text],#customer input[type=email],#customer input[type=password],#account  input[type=text],#account input[type=email],#account input[type=password]{width:305px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}

#address_form_new input[type=text]{margin-bottom:0px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}

table.customer_address_table{width:400px;table-layout:fixed;}
.customer_address_table .label{vertical-align:bottom;width:25%;}
.customer_address_table .value{width:75%;padding:20px 0px 0px 10px;}
.customer_address_table .label > label{font-weight:normal;}
.customer_address_table .value > input[text],.customer_address_table .value > select{margin:0px;width:100%;display:block;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}




/* =============================================== */
/* =               MEDIA QUERIES                 = */
/* =============================================== */

/* Smaller than standard 960 (devices and browsers) */
@media only screen and (max-width: 959px) {
	.overview-third{width:278px;}
	.f-copy{clear:both}
}

/* All Mobile Sizes (devices and browser) */
@media only screen and (max-width: 767px) {
  
	.row-one,.prod-grid-item,.first-prod,.last-prod{margin:0px;}
	h1 {font-size:40px;}
	h2 {font-size:24px;}  
	.padding-top{padding-top:20px;}  
  
  	/* NAV */
	.hide-mobile{display:none !important;}
	nav.large{display:none !important;}
	.mobile{display:block;}
	#main_navigation{width:100%;}
	#main > .twelve{padding-right:0px;}

	.nav-bar{height:47px;display:none;}
	.m-nav-bar{display:inline-block;height:52px;background-color:grey}
	.nav-item{width:100%} 
  
  
  	/* FOOTER */
	.overview-third{width:100%;margin:20px 0px 0px 0px;}
	.o-first{margin-top:0px;}
	.f-copy{clear:both}


  	/* CART */
	#cartform td{width:100%;display:block;}
	#cartform th, #cartform .total-text{display:none;}
	#cartform .qty:before{content:"Quantity";float:left;width:25%;text-align:right;margin-right:5%;}
	#cartform .price:before{content:"Price";float:left;width:25%;text-align:right;margin-right:5%;}
	#cartform .qty, #cartform .price{clear:both;}
	#cartform .price {padding-top:0px;text-align:left;} 
	#cartform .remove{padding-top:0px;text-align:right;}
	#cartform .total-price{font-size:20px;padding-left:0px;}
  
  
	.flexslider h5{font-weight:normal;font-size:13px;}
	
  	/* CUSTOMER */
	#customer input[type=text],#customer input[type=email],#customer input[type=password], #account  input[type=text],#account input[type=email],#account input[type=password]{width:100%;}
  	#addresses input[type=text], #addresses select{width:100%;}	
  	table.customer_address_table{width:100%;}
  	.full.btn{width:100%;margin:5px 0px;}
  	#sidebar{display:none;}
  
  	/* SEARCH */
  	#search h3{text-align:center;}
  	.result-image{margin:0 0 20px 0;width:100%;max-width:100%;}
  	.result-image img{max-width:100%;max-height:300px;margin:0 auto;display:block;}
}

/* Tablet Portrait size to standard 960 (devices and browsers) */
@media only screen and (min-width: 768px) and (max-width: 959px) {
	.overview-third{width:214px;}
	.f-copy{clear:both}
}

/* =================================================== */
/* =                   FLEXSLIDER                    = */
/* =================================================== */


/* Browser Resets */
.flex-container a:active,
.flexslider a:active,
.flex-container a:focus,
.flexslider a:focus  {outline: none;}
.slides,
.flex-control-nav,
.flex-direction-nav {margin: 0; padding: 0; list-style: none;} 

/* FlexSlider Necessary Styles
*********************************/ 
.flexslider {margin: 0; padding: 0;}
.flexslider .slides > li {display: none; -webkit-backface-visibility: hidden;} /* Hide the slides before the JS is loaded. Avoids image jumping */
.flexslider .slides img {width: 100%; display: block;}
.flex-pauseplay span {text-transform: capitalize;}

/* Clearfix for the .slides element */
.slides:after {content: "."; display: block; clear: both; visibility: hidden; line-height: 0; height: 0;} 
html[xmlns] .slides {display: block;} 
* html .slides {height: 1%;}

/* No JavaScript Fallback */
/* If you are not using another script, such as Modernizr, make sure you
* include js that eliminates this class on page load */
.no-js .slides > li:first-child {display: block;}

/* FlexSlider Default Theme
*********************************/
.flexslider {margin: 0 0 60px; background: #fff; border: 4px solid #fff; position: relative; -webkit-border-radius: 4px; -moz-border-radius: 4px; -o-border-radius: 4px; border-radius: 4px; zoom: 1;}
.flex-viewport {max-height: 2000px;}
.loading .flex-viewport {max-height: 300px;}
.flexslider .slides {zoom: 1;}

.slides > li {margin-right: 10px;}

.flexslider-container{margin-top:10px;}
.flexslider-outer{text-align:center;clear:both}
div.flexslider-outer > div.twelve{display:inline-block;float:none}
ul.slides > li {box-sizing:border-box;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;}
.flexslider-outer > h5{padding:10px;border-style: solid; border-width:1px 0px 1px 0px; border-color:#ddd;}

/* Direction Nav */
.flex-direction-nav {*height: 0;}
.flex-direction-nav a {width: 35px; height: 35px; margin: -17px 0 0; display: block; display:none;background: url(http://cdn.shopify.com/s/files/1/0233/9575/t/5/assets/slider.png?4846) no-repeat 0 0; position: absolute; top: 50%; z-index: 10; cursor: pointer; text-indent: -9999px; opacity: 0; -webkit-transition: all .3s ease;}
.flex-direction-nav .flex-next {background-position: 100% 0; right: -36px; }
.flex-direction-nav .flex-prev {left: -80px;}
.flexslider:hover .flex-next {opacity: 0.8; right: 5px;}
.flexslider:hover .flex-prev {opacity: 0.8; left: 5px;}
.flexslider:hover .flex-next:hover, .flexslider:hover .flex-prev:hover {opacity: 1;}
.flex-direction-nav .flex-disabled {opacity: .3!important; filter:alpha(opacity=30); cursor: default;}

/* Control Nav */
.flex-control-nav {width: 100%; position: absolute; margin-top: 10px; text-align: center;}
.flex-control-nav li {margin: 0 6px; display: inline-block; zoom: 1; *display: inline;}
.flex-control-paging li a {width: 11px; height: 11px; display: block; background: #666; background: rgba(0,0,0,0.5); cursor: pointer; text-indent: -9999px; -webkit-border-radius: 20px; -moz-border-radius: 20px; -o-border-radius: 20px; border-radius: 20px; box-shadow: inset 0 0 3px rgba(0,0,0,0.3);}
.flex-control-paging li a:hover { background: #333; background: rgba(0,0,0,0.7); }
.flex-control-paging li a.flex-active { background: #000; background: rgba(0,0,0,0.9); cursor: default; }

.flex-control-thumbs {margin: 5px 0 0; position: static; overflow: hidden;}
.flex-control-thumbs li {width: 25%; float: left; margin: 0;}
.flex-control-thumbs img {width: 100%; display: block; opacity: .7; cursor: pointer;}
.flex-control-thumbs img:hover {opacity: 1;}
.flex-control-thumbs .flex-active {opacity: 1; cursor: default;}

@media screen and (max-width: 860px) {
	.flex-direction-nav .flex-prev {opacity: 1; left: 0;}
	.flex-direction-nav .flex-next {opacity: 1; right: 0;}
}
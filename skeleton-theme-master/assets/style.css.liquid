/* ================ */
/* =  Global CSS  = */
/* ================ */

html,body{height:100%;}

h1, h2, h3, h4, h5, h6 {color:{{settings.header_color}};
font-family:{{ settings.header_font | remove: 'Google+' | replace: '+', ' ' }};
}
body{background-color:{{settings.shop_bg_color}};}
body,p,strong{
	font-family: {{ settings.base_font | remove: 'Google+' | replace: '+', ' ' }}; 
	color:{{settings.body_font_color}};
}
img{max-width:100%; height: auto;border-radius:2px;}
a{color:{{settings.link_color}};}
a:hover{opacity:0.7;}

/* Hiding placeholder text on focus in webkit, thanks to http://www.kolodvor.net/2012/03/23/webkit-placeholder-attribute-behavior */

input:focus::-webkit-input-placeholder, textarea:focus::-webkit-input-placeholder { color:transparent; }

/* Contain floats: nicolasgallagher.com/micro-clearfix-hack/ */ 

.clearfix:before, .clearfix:after { content: ""; display: table; }
.clearfix:after { clear: both; }
.clearfix { zoom: 1; }     
.hidden { display: none; }
.left { float: left; }
.right { float: right; }
.boxify{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;} 
.table{display: table !important;}
.table-cell { display: table-cell !important;  vertical-align: middle;}
.no-border{border: none !important; }

hr.divider{width:100%;display:block;height:1px;border-top: 1px solid #e8e8e8;margin:30px 0;}
.divider.large{margin:40px 0;}
.show-mobile-only{display:none !important;}


/**** Buttons ****/

.btn{background-color:transparent;font-family: Helvetica, Arial, sans-serif;line-height: 40px; padding:10px 15px;border:1px solid {{settings.shop_btn_color}};-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;font-size:14px;font-weight:bold;display:inline-block;}
.btn:hover{opacity:0.8;}
.btn-colour{color: #fff !important;background-color: {{settings.shop_btn_color}};}
.btn.subtle{color: {{settings.shop_btn_color}} !important;background-color:transparent;border:1px solid {{settings.shop_btn_color}};margin-left:7px;}
a.btn{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;text-align:center;}
input.btn[type="submit"]{font-weight:bold;}


/* ================ */
/* = theme.liquid = */
/* ================ */


/**** Header ****/


.header{margin-bottom:0;background:#000; height:50px; z-index:9999; width:100%;}
.header-title{font-weight: bold; margin-bottom:0; height:50px; line-height:50px;  }
.header-title a,.header-title a:hover{color:#fff;}
.header-logo{display:block;margin:10px auto;}

/**** Banner ****/

.banner{margin-top:15px;margin-bottom:0px;margin-left:0px;margin-right:0px;width:100%;border-bottom:1px solid {{settings.border_color}};}
.banner h6{margin-bottom:15px;text-align:center;}


/**** Nav Bar ****/

.stuck{position:fixed;top:0;z-index:999;}

nav{font-weight:bold;}
nav a{color:{{settings.nav_link_color}};}
nav li, nav ul{margin-bottom:0;}
.nav-option a{font-size:19px !important;}
.nav-divider{background:{{settings.nav_link_color}}; height: 50px; width: 1px; opacity: 0.3;}
.dropdown ul{display:none;background-color:white;border: 1px solid {{settings.border_color}}};font-weight:normal;}
.dropdown ul li{width:250px;padding: 15px 18px 15px 18px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;margin-bottom:0px;}
.m-nav-bar{display:none;}
.mobile{display:none;}


.container {padding:0;}
#template{padding-bottom:60px;}

#main{margin-top:60px;}
#main > #sidebar, #main > .twelve, #main > .row{margin: 60px 0;}
#main > .twelve{padding-right:20px;}




/**** Side Bar ****/

#sidebar h6 {margin-bottom:5px;line-height:1.4em;}
#sidebar .sidebar-post{margin-bottom:30px; padding-bottom:30px; border-bottom:1px solid {{settings.border_color}};}



/**** Footer  ****/
#overview{ border-top:1px solid {{settings.border_color}}; border-bottom:1px solid {{settings.border_color}};  padding:30px 0; margin-bottom:30px; }
#overview h4{margin-bottom:15px;}
#overview .last{margin-right:0;}
#overview .one-third{margin-bottom:30px;}
#overview h1, #overview h2, #overview h3, #overview h4,#overview h5, #overview h6{color:{{settings.footer_header_color}};}
#overview p{color:{{settings.footer_text_color}}}
.f-copy{text-align:center;}

.footer-nav a{display:inline-block; width:47%;}
.footer-news input[type="text"]{width:97%;-webkit-box-sizing: border-box; border: 1px solid #DDDBDB; box-sizing: border-box; vertical-align:top; display:inline-block; width:62.8%;}
.footer-news input[type="submit"]{display:inline-block;margin-bottom:0px; width:32.5%;}
.footer-inner p, .footer-inner a {font-size:12px;}
.footer-inner a:hover{text-decoration:underline;}
#footer-payment-methods{text-align:center; margin-bottom:15px;}
.f-links a{display:inline-block; margin-right:15px;}

/**** Footer social icons ****/

.social-container{display:inline-block;}
.social-container {display:inline-block; }
.social{font-size:25px; margin-right:10px;}



/* Custom select */

.custom-select{margin:0 0 10px 10px; background-color:white; color:green; border:1px solid #ccc;padding:5px 5px 5px 10px;border-radius:5px;color:#777;}
.custom-select:after{font-family: FontAwesome; content: "\F078";padding-right:5px;}
.custom-selectHover{border: 1px solid #aaa; color: #444;}
.custom-selectFocus{border: 1px solid #aaa; color: #444;-webkit-box-shadow: 0 0 3px rgba(0,0,0,.2);box-shadow: 0 0 3px rgba(0,0,0,.2);}



/* ================ */
/* = index.liquid = */
/* ================ */

/*** Fullscreen banner ***/

.table{display:table;}
.vert-table-cell{display:table-cell;vertical-align:middle;text-align:center;}

.fullscreen-banner.outer{display:block;width:100%;height:100%;background-color:{{settings.fullscreen_bg_color}};background-size:cover;
  {% if settings.enable_fullscreen_bg_image %}background-image:url("{{'fullscreen_bg_image.png'|asset_url}}");{% endif %}background-attachment:fixed;overflow:hidden;}
.fb-inner{height:100%;width:100%;}
.fb-content{height:100%;padding-right:10%;padding-left:10%;width:100%;}
.fb-content-container{height:100%;}
.fb-content img{display:block;max-width:80%;display:inline-block;}
.fb-content h1{font-weight:normal;line-height:1.3em;color:{{settings.fullscreen_header_color}};font-family:{{ settings.fullscreen_header_font | remove: 'Google+' | replace: '+', ' ' }};max-width:600px;margin:0 auto;}

@media only screen and (min-height: 768px){
  .fb-content h1{margin-top:60px;font-size:40px;}
}

@media only screen and (min-height: 600px) and (max-height: 797px){
  .fb-content h1{margin-top:40px;font-size:30px;}
}

@media only screen and (min-height: 510px) and (max-height: 629px) {
 .fb-content h1{margin-top:20px;font-size:25px;}
}

@media only screen and (max-height: 509px){
  .fb-content h1{margin-top:10px;font-size:20px;}
}

/*** Featured product ***/


#feat-coll-header{margin-bottom:20px;padding: 10px;border-style: solid;border-width: 1px 0px 1px 0px;border-color: {{settings.border_color}};text-align:center}

/* ================== */
/* = product.liquid = */
/* ================== */

/*** Product images ***/

#product-photos{margin-bottom:30px;}
#product-photos a:hover{opacity:1;}
#product-photo-container{max-width:100%;max-height:500px;margin-bottom:10px;}

#product-photo-container img{max-width:100%; max-height:500px;display:block;margin:0 auto;}
#product-photo-thumbs li{line-height:0;width:146px;max-height: 292px;margin-right:10px; }
#product-photo-thumbs img{margin:0 auto;display:block;}

.product-last-thumb{margin-right:0 !important;}
.mobile-title{display:none; text-align:center; }


/*** Product details ***/
.add-container label{font-weight:bold;}
input#quantity{height:38px;width:50px;margin-right:10px;padding:5px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
#product-add,#quantity{display:inline-block;float:left;}
#product-details{margin: 0 0 60px 0;}
#product-variants{ border-bottom: 1px solid {{settings.border_color}}; margin-bottom:30px;}

.selector-wrapper{margin-bottom:30px;}
.selector-wrapper label, .selector-wrapper select,.selector-wrapper span{margin:0;}
.selector-wrapper label{font-weight:bold;margin-bottom:10px;}
#product-price{margin-bottom:30px;}
.product-price{font-size:36px;font-weight:bold; vertical-align:text-top; }
.product-compare-price{font-size:18px;font-weight:normal;padding-left:3px;color:grey; vertical-align:bottom;}
.add-container .selector-wrapper{display:inline;}
.add-container + hr{margin-top:30px;margin-bottom:30px;}


/* ==================== */
/* = collection.liquid = */
/* ==================== */

.product-tags{text-align:right;}   

.collection-loop li.coll-grid-item,
.product-loop li.prod-grid-item{margin-bottom:20px;margin-left:20px;margin-right:0px;}

li.coll-grid-item.first-in-row,
li.prod-grid-item.first-in-row{clear:left;}

.coll-grid-item img,
.prod-grid-item img{margin: 0 auto;display: block;}

ul.grid li { float: left; list-style-type: none;}

.coll-image-wrap,
.prod-grid-item .prod-image-wrap{padding:0px;max-height:500px;position:relative;line-height:0;}

.info-container{-webkit-border-radius:2px;border-radius:2px;display:none;position:absolute;top:0px;left:0px;width:100%;height:100%;background-color:rgb(25, 20, 22);background-color:rgba(0,0,0,0.9);}
.info-vertical-center{display:table;padding:0px;height:100%;width:100%;}

.collection-info,
.prod-grid-item .product-info{vertical-align:middle;display:table-cell;width:100%;text-align:center;line-height:1;padding-top:20px;}

